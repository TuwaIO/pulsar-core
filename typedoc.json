{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "packages/pulsar-core/src/index.ts",
    "packages/pulsar-evm/src/index.ts"
  ],
  "entryPointStrategy": "expand",
  "tsconfig": "tsconfig.json",
  "plugin": [
    "typedoc-plugin-markdown",
    "typedoc-plugin-react"
  ],
  "out": "apps/docs/src/content/apiReference",
  "readme": "none",
  "githubPages": false,
  "hideGenerator": true,
  "cleanOutputDir": true,
  "categorizeByGroup": true,
  "hideBreadcrumbs": true,
  "pageTitleTemplates": {
    "member": "{name}"
  }
}